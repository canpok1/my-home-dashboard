{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "dotenv -e ../../.devcontainer/db.env nuxt dev",
    "generate": "dotenv -e ../../.devcontainer/db.env nuxt generate",
    "preview": "dotenv -e ../../.devcontainer/db.env nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "nuxi cleanup",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "format-fix": "prettier --write --ignore-path .gitignore .",
    "typecheck": "nuxi typecheck",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/devtools": "^1.0.8",
    "@sidebase/nuxt-auth": "^0.6.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@vitest/coverage-v8": "^1.3.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "~3.6",
    "sass": "^1.71.0",
    "timezone-mock": "^1.3.6",
    "typescript": "^5.3.3",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.3.0",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27",
    "vuetify": "^3.5.4",
    "zod": "^3.22.4"
  }
}
