{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "dotenv -e ../../.devcontainer/db.env nuxt dev",
    "generate": "dotenv -e ../../.devcontainer/db.env nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "nuxi clean",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "format-fix": "prettier --write --ignore-path .gitignore .",
    "typecheck": "nuxi typecheck",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@types/chart.js": "^2.9.39",
    "@types/node": "^20",
    "@vitest/coverage-v8": "^1.3.0",
    "chart.js": "^4.4.1",
    "nuxt": "3.10",
    "sass": "^1.71.0",
    "timezone-mock": "^1.3.6",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.3.0",
    "vue-tsc": "^1.8.27",
    "vuetify": "^3.3.23"
  }
}
