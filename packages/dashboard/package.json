{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "dotenv -e ../../.devcontainer/db.env nuxt dev",
    "generate": "dotenv -e ../../.devcontainer/db.env nuxt generate",
    "preview": "dotenv -e ../../.devcontainer/db.env nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "nuxi cleanup",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "format-fix": "prettier --write --ignore-path .gitignore .",
    "typecheck": "nuxi typecheck",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/devtools": "^2.7.0",
    "@sidebase/nuxt-auth": "^0.10.1",
    "@types/node": "^20",
    "@vitest/coverage-v8": "^1.6.1",
    "jose": "^5.10.0",
    "nuxt": "~3.16",
    "sass": "^1.97.1",
    "timezone-mock": "^1.3.6",
    "typescript": "^5.9.3",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^1.6.1",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "vue-tsc": "^2.2.12",
    "vuetify": "^3.11.6",
    "zod": "^3.25.76"
  }
}
